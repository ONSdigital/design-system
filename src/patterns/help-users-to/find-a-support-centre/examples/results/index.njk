---
title: "Example: Find a support centre results"
layout: none
---
{% extends "layout/_template.njk" %}

{% from "components/collapsible/_macro.njk" import onsCollapsible %}
{% from "components/lists/_macro.njk" import onsList %}
{% from "components/external-link/_macro.njk" import onsExternalLink %}

{% set pageConfig = {
    "theme": "census",
    "absoluteUrl": pageInfo.url,
    "headMeta": {
        "description": "Census 2021 is a UK wide population survey."
    },
    "header": {
        "orgLogoHref": "#0",
        "titleLogo": "census-logo-en",
        "titleLogoClasses": 'ons-u-mt-s@m ons-u-mb-s@m',
        "titleLogoHref": "#0",
        "titleLogoAlt": "Census 2021"
    },
    "language": {
        "languages": [
            {
                "url": "#0",
                "ISOCode": "en",
                "text": "English",
                "current": true
            },
            {
                "url": "#0",
                "ISOCode": "cy",
                "text": "Cymraeg",
                "current": false
            }
        ]
    },
    "footer": {}
} %}

{% block main %}

    <h1 class="ons-u-mt-l ons-u-fs-xxl">Support centres near <em class="ons-highlight">S1 1XN</em></h1>

    <div class="ons-u-pb-m ons-u-pt-m">

        <h2 class="ons-u-fs-l ons-u-mb-xs">Sheffield Central Library</h2>
        <span class="ons-u-mb-s ons-distance">0.7 miles away</span>

        <p>First Floor, Central Library <br> Surrey Street <br> Sheffield <br> S1 1XZ</p>

        <p>{{
            onsExternalLink({
                "url": "https://goo.gl/maps/X48tjZkXA4RaN3LL7",
                "linkText": 'Find<span class="ons-u-vh"> Sheffield Central Library</span> on Google Maps'
            })
        }}</p>

        {{
            onsList({
                "variants": 'bare',
                "itemsList": [
                    {
                        "text": 'Telephone: 01142 734 712'
                    },
                    {
                        "text": 'Email: <a href="mailto:information.library@sheffield.gov.uk">information.library@sheffield.gov.uk</a>'
                    }
                ]
            })
        }}

        {% call onsCollapsible({
            "id": 'collapsible-1',
            "title": 'Opening times and facilities',
            "titleTag": 'h3',
            "button": {
                "close": 'Hide this'
            }
        }) %}

        <h4 class="ons-u-fs-r--b ons-u-mb-xs">Opening times</h4>

        {{
            onsList({
                "itemsList": [
                    {
                        "text": 'Monday to Friday, 9am to 4pm'
                    },
                    {
                        "text": 'Saturday, 9am to 5pm'
                    },
                    {
                        "text": 'Sunday, 9am to 5pm'
                    },
                    {
                        "text": 'Census weekend (20 â€“ 21 March), 9am to 5pm'
                    }
                ]
            })
        }}
        
        <h4 class="ons-u-fs-r--b ons-u-mb-xs">Centre facilities</h4>

        {{
            onsList({
                "itemsList": [
                    {
                        "text": 'Public parking'
                    },
                    {
                        "text": 'Level access into building entrance'
                    },
                    {
                        "text": 'Wheelchair access'
                    },
                    {
                        "text": 'Staff are disability aware'
                    },
                    {
                        "text": 'Hearing loop system'
                    }
                ]
            })
        }}

        {% endcall %}

    </div>

    <div class="ons-u-pb-m ons-u-pt-m ons-u-bt">

        <h2 class="ons-u-fs-l ons-u-mb-xs">University of Sheffield</h2>
        <span class="ons-u-mb-s ons-distance">1.7 miles away</span>

        <p>The University of Sheffield <br> Western Bank <br> Sheffield <br> S10 2TN</p>

        <p>{{
            onsExternalLink({
                "url": "https://goo.gl/maps/SEaVUjW2KY9wDoPE8",
                "linkText": 'Find<span class="ons-u-vh"> University of Sheffield</span> on Google Maps'
            })
        }}</p>

        {{
            onsList({
                "variants": 'bare',
                "itemsList": [
                    {
                        "text": 'Telephone: 0114 222 2000'
                    },
                    {
                        "text": 'Email: <a href="mailto:ask@sheffield.ac.uk">ask@sheffield.gov.uk</a>'
                    }
                ]
            })
        }}

        {% call onsCollapsible({
            "id": 'collapsible-2',
            "title": 'Opening times and facilities',
            "titleTag": 'h3',
            "button": {
                "close": 'Hide this'
            }
        }) %}

        <h4 class="ons-u-fs-r--b ons-u-mb-xs">Opening times</h4>

        <p>Monday to Friday, 8am to 6pm</p>
        
        <h4 class="ons-u-fs-r--b ons-u-mb-xs">Centre facilities</h4>

        {{
            onsList({
                "itemsList": [
                    {
                        "text": 'Public parking'
                    },
                    {
                        "text": 'Level access into building entrance'
                    },
                    {
                        "text": 'Wheelchair access'
                    }
                ]
            })
        }}

        {% endcall %}

    </div>

{% endblock %}
