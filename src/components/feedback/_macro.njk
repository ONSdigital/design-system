{% macro onsFeedback(params) %}

    <div id="params.id" class="feedback">
        <div class="feedback__body">
            <h2>{{ params.header }}</h2>
            <p>{{ params.content }}</p>
            <div>
                <a href="{{ params.url }}">{{- params.linkText -}}</a>
            </div>
        </div>
        <div class="feedback__footer">
            <svg class="svg-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 25.26 25.28">
                <polyline points="25.26 0 21 0 21 0 3 18 3 0 0 0 0 25.28"></polyline>
                <polygon points="3 0 21 0 3 18 3 0" style="fill:#fff"></polygon>
            </svg>
        </div>
    </div>
{% endmacro %}
