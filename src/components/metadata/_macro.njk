        onsDescriptionList({
            "id": params.id,
            "classes": params.classes,
            "descriptionListLabel": params.metadataLabel,
            "termCol": params.termCol,
            "descriptionCol": params.descriptionCol,
            "itemsList": params.itemsList
        })
