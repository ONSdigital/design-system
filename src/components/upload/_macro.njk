{% macro onsUpload(params) %}
    {% from "components/input/_macro.njk" import onsInput %}

    {{ onsInput({
        "id": params.id,
        "type": "file",
        "label": params.label,
        "classes": "input--upload",
        "accept": params.accept,
        "name": params.name,
        "attributes": params.attributes,
        "listeners": params.listeners
    }) }}
{% endmacro %}
