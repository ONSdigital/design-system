{# all on one line to prevent extra underline when text comes after link #}
{% macro onsExternalLink(params) %}
    <a href="{{- params.url -}}" class="external-link" target="_blank" rel="noopener">{{- params.linkText -}}<svg><use xlink:href="/img/icons/icons--external-link.svg#external-link"></use></svg></a>
{% endmacro %}
