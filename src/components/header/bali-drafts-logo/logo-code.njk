var logos = [logo1, logo2, logo3]

{% for logo in logos %}

    {% if params.mastheadLogoStackedArray.[logo].logoURL %} 
        <a class="ons-header__org-logo-link" href="{{ params.mastheadLogoStackedArray.[logo].logoURL }}">
    {% endif %}

    {% if params.mastheadLogoStackedArray.logo1.logoImage != "ONS Logo" %}
        {{ params.mastheadLogoStackedArray.logo1.logoImage | safe }}
    {% else %}
    <script>console.log('{{params.mastheadLogoStackedArray.logo1.logoImage}}')</script>

        {{-
            onsIcon({
                "iconType": 'ons-logo-stacked-' + currentLanguageISOCode,
                "altText": 'Office for National Statistics logo'
            })
        -}}
    {% endif %}
    
    {% if params.mastheadLogoStackedArray.logo1.logoURL %}
        </a>
    {% endif %}

    {% x+=1 %}
{% end %}




