{% if params.element %}
    {% set listEl = params.element | lower %}
{% else %}
    {% set listEl = 'ul' %}
{% endif %}

<{{listEl}} {% if params.id %}id="{{ params.id }}"{% endif %} class="list {{ params.classes }}">
    {% for item in params.items %}
        <li class="list__item {{ item.listclass }}">
            {% if item.href %}
                {% if item.index %}{{ loop.index }}) {% endif %} <a href="{{ item.href }}"{% if item.classes %} class="{{ item.classes }}"{% endif %}{% if item.target %} target="{{ item.target }}"{% endif %}>
                    {{ item.text }}
                </a>
            {% else %}
                {{ item.text }}
            {% endif %}
        </li>
    {% endfor %}
</{{listEl}}>
